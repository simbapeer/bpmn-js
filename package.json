{
  "name": "bpmn-js",
  "version": "0.22.1",
  "description": "A bpmn 2.0 toolkit and web modeler",
  "scripts": {
    "all": "grunt",
    "dev": "grunt auto-test",
    "distro": "webpack",
    "test": "grunt test",
    "lint": "grunt lint",
    "build": "browserify -o dist/bpmn-viewer.js -s BpmnViewer -e lib/Viewer.js -g babelify -p browserify-derequire -p bundle-collapser/plugin --igv global --no-builtins",
    "minify": "uglifyjs -cm -o dist/bpmn-viewer.min.js dist/bpmn-viewer.js",
    "a": "npm run build && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js"
  },
  "keywords": [
    "bpmn",
    "bpmn-js",
    "toolkit",
    "web modeler",
    "modeler",
    "modeling",
    "process modeling"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-derequire": "^0.9.1",
    "bundle-collapser": "^1.1.1",
    "chai": "^3.5.0",
    "chai-match": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-plugin-mocha": "^4.9.0",
    "grunt": "^0.4.4",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-eslint": "^19.0.0",
    "grunt-karma": "^2.0.0",
    "grunt-release": "^0.7.0",
    "jsondiffpatch": "^0.1.26",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-spec-reporter": "0.0.31",
    "load-grunt-tasks": "^0.3.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.4.1",
    "mocha-test-container-support": "0.2.0",
    "phantomjs-prebuilt": "^2.1.12",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "source-map-concat": "^1.0.0",
    "stringify": "^5.1.0",
    "time-grunt": "^0.3.2",
    "uglify-js": "^3.1.4",
    "watchify": "^3.7.0",
    "webpack": "^3.8.1",
    "webpack-babel-external-helpers": "^0.1.0",
    "webpack-babel-external-helpers-2": "^3.0.2",
    "webpack-common-shake": "^1.5.3"
  },
  "dependencies": {
    "bpmn-moddle": "github:bpmn-io/bpmn-moddle#ES2015",
    "diagram-js": "github:bpmn-io/diagram-js#ES2015",
    "diagram-js-direct-editing": "^0.19.0",
    "diagram-js-origin": "^0.15.2",
    "ids": "^0.2.0",
    "inherits": "^2.0.1",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.4",
    "min-dom": "^0.3.0",
    "object-refs": "^0.1.1",
    "tiny-svg": "^0.1.2"
  }
}
